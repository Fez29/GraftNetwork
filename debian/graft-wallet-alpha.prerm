#!/bin/sh

set -e

case "$1" in
  remove|deconfigure)
    update-alternatives --remove graft-wallet-cli /usr/bin/graft-wallet-cli.alpha
    update-alternatives --remove graft-wallet-rpc /usr/bin/graft-wallet-rpc.alpha
    ;;

  upgrade|failed-upgrade)
    ;;

  *)
    echo "prerm called with unknown argument '$1'" >&2
    exit 0
    ;;
esac

exit 0
